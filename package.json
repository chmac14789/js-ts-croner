{
  "name": "croner",
  "version": "3.0.40",
  "description": "Isomorphic JavaScript cron parser and scheduler.",
  "author": "Hexagon <github.com/hexagon>",
  "contributors": [
    {
      "name": "Pehr Boman",
      "email": "github.com/unkelpehr"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hexagon/croner"
  },
  "bugs": {
    "url": "https://github.com/hexagon/croner/issues"
  },
  "keywords": [
    "cron",
    "parser",
    "croner",
    "sheduler",
    "timer",
    "isomorphic"
  ],
  "scripts": {
    "test": "npm run test:mocha",
    "test:mocha": "mocha",
    "test:lint": "eslint ./**/*.js ./**/*.cjs",
    "test:lint:fix": "eslint --fix  ./**/*.js ./**/*.cjs",
    "build": "npm run test:lint && npm run build:typings && npm run build:webpack && npm run test:mocha && npm run build:finalize",
    "build:webpack": "webpack",
    "build:typings": "tsc",
    "build:finalize": "echo \"All good\""
  },
  "type": "module",
  "main": "./dist-legacy/croner.cjs",
  "browser": "./dist/croner.min.js",
  "module": "./src/croner.js",
  "exports": {
    ".": {
      "import": "./src/croner.js",
      "require": "./dist-legacy/croner.cjs",
      "browser": "./dist/croner.min.js"
    },
    "./minified": {
      "import": "./dist/croner.min.mjs",
      "require": "./dist/croner.min.js",
      "browser": "./dist/croner.min.js"
    }
  },
  "types": "types/croner.d.ts",
  "devDependencies": {
    "eslint": "^7.32.0",
    "mocha": "^9.1.2",
    "should": "^13.2.3",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0"
  },
  "license": "MIT"
}
